repeat wait() until game:IsLoaded()

local loading = {}
for i,v in next,game.CoreGui:GetChildren() do
if v.Name == "Loader" then
v:Destroy()
end
end
function loading:LoaderCreate(Name,Owner,Time)
    local Bars = {}
local Loader = Instance.new("ScreenGui")
local Canvas = Instance.new("Frame")



Loader.Name = "Loader"
Loader.Parent = game.CoreGui
Loader.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Canvas.Name = "Canvas"
Canvas.Parent = Loader
Canvas.AnchorPoint = Vector2.new(0.5, 0.5)
Canvas.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Canvas.BackgroundTransparency = 1
Canvas.BorderSizePixel = 0
Canvas.ClipsDescendants = true
Canvas.Position = UDim2.new(0.5, 0, 0.5, 0)
Canvas.Size = UDim2.new(0, 400, 0, 250)

local Posing = 0
local FirstBarColor = 200


function Bars:AddBar()
	for i = 1,6 do
local curpos = 0
if i == 1 then
local curpos = 0
else
        Posing += 50
        curpos = Posing
end
        FirstBarColor -= 15
        local BarColor = FirstBarColor
		local Bar = Instance.new("Frame")
		Bar.Name = "Bar"..i
		Bar.Parent = Canvas
		Bar.BackgroundColor3 = Color3.fromRGB(0,BarColor - 50, BarColor)
		Bar.BorderSizePixel = 0
		Bar.Position = UDim2.new(0, 0, 1.3, 0)
		Bar.Size = UDim2.new(1, 0, 0, 50)
		
		Bar:TweenPosition(UDim2.new(0,0,0,curpos),"InOut","Quart",1,true,nil)
spawn(function()
wait(2)
Bar:TweenPosition(UDim2.new(0,0,1.3,0),"InOut","Quart",1,true,nil)
wait(1)
Loader:Destroy()
end)
if i == 3 then
            local TextLabel = Instance.new("TextLabel")

            TextLabel.Parent = Bar
            TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.BackgroundTransparency = 1.000
            TextLabel.BorderSizePixel = 0
            TextLabel.Size = UDim2.new(1, 0, 0, 50)
            TextLabel.Font = Enum.Font.IndieFlower
            TextLabel.Text = Name 
            TextLabel.TextColor3 = Color3.fromRGB(200,200,200)
            TextLabel.TextSize = 56.000
        end
        if i == 4 then
            local TextLabel = Instance.new("TextLabel")

            TextLabel.Parent = Bar
            TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel.BackgroundTransparency = 1.000
            TextLabel.BorderSizePixel = 0
            TextLabel.Size = UDim2.new(1, 0, 0, 50)
            TextLabel.Font = Enum.Font.IndieFlower
            TextLabel.Text = Owner 
            TextLabel.TextColor3 = Color3.fromRGB(125,125,125)
            TextLabel.TextSize = 32.000
        end
wait()
end
end
return Bars;
end

local mainload = loading:LoaderCreate("Ocerium","Made By SALFIN#2470",2)
local barsCreating = mainload:AddBar()


wait(2)




--Variables
local itemsPath = game:GetService("Workspace").Asda;
local plr = game.Players.LocalPlayer

local lib = loadstring(game:HttpGet("https://github.com/slf0Dev/SLF-library-performance/raw/main/lib%20core"))()

--Tables

_G.Toggles = {
    ["ItemFarm"] = false
}

_G.Pickups = {
    ["Banknote"] = true,
    ["Arrow"] = true,
    ["Requiem Arrow"] = true,
    ["Rokakaka Fruit"] = true,
    ["Tricky"] = true,
    ["NightmareDiary"] = true,
    ["Shiny Diary"] = true,
    ["Gold!"] = true,
    ["Frog"] = true,
    ["DIO's Diary"] = true,
    ["Vampire Mask"] = true,
    ["Water"] = true,
    ["TrollFace"] = true,
    ["Potato"] = true
    }
    
    --main code
    function FarmItems()
        while _G.Toggles["ItemFarm"] do wait(0.05)
            for i,v in next,itemsPath:GetChildren() do
                for _,item in next,_G.Pickups do
                    if v.Handle and v.Parent == itemsPath and _ == v.Name and item == true then
                        plr.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
                    end
                end
            end
        end
    end

    -- Creating GUI

    local MainWindow = lib:Window("Ocerium")
    
    local ItemFolder = MainWindow:Folder("Items")

    local FarmingToggle = ItemFolder:Toggle("Items Farm",function(bool)
        _G.Toggles["ItemFarm"] = bool
        spawn(FarmItems)
    end,false)

    local FilterWindow = lib:Window("Items Filter")

    local FilterFolder = FilterWindow:Folder("Filter items")

    for i,v in next,_G.Pickups do
        local filterToggle = FilterFolder:Toggle(i,function(bool)
            _G.Pickups[i] = bool
        end,true)
    end
